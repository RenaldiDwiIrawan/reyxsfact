---
const items = ["./tech-stack/mongoDB.jpg"];
---

<div class="mongoDBComponen">
	{
		items.map((item) => (
			<img
				loading="lazy"
				class="mongoDBComponen"
				src={item}
				alt="logo mongoDB"
			/>
		))
	}
</div>

<style>
	.mongoDBComponen {
		display: flex;
		justify-content: center;
		gap: 12px;
		cursor: auto;
	}
	@media (max-width: 768px) {
		.mongoDBComponen {
			display: flex;
			width: 55%;
			justify-content: left;
			flex-direction: row;
			gap: 12px;
		}
	}
	.mongoDBComponen {
		height: 50px;
		width: auto;
	}
</style>
